<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TinhThueServlet</servlet-name>
        <servlet-class>controller.TinhThueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DongThueServlet</servlet-name>
        <servlet-class>controller.DongThueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DangNhapServlet</servlet-name>
        <servlet-class>controller.DangNhapServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DangKiServlet</servlet-name>
        <servlet-class>controller.DangKiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DangKiMSTServlet</servlet-name>
        <servlet-class>controller.DangKiMSTServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TinhThueServlet</servlet-name>
        <url-pattern>/TinhThueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DongThueServlet</servlet-name>
        <url-pattern>/DongThueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DangNhapServlet</servlet-name>
        <url-pattern>/DangNhapServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DangKiServlet</servlet-name>
        <url-pattern>/DangKiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DangKiMSTServlet</servlet-name>
        <url-pattern>/DangKiMSTServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
